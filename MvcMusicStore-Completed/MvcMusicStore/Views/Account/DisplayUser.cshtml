@model IEnumerable<MvcMusicStore.Models.Order>

@{
    ViewBag.Title = "DisplayUser";
}

<h2>Your Order Details</h2>

<div id="order-table">
    <table>
        <tr>
            <th>Order ID</th>
            <th>Order Date</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Postal Code</th>
            <th>Country</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Total</th>
        </tr>

        @foreach (var order in Model)
        { 
            <tr>
                <td>@order.OrderId</td>
                <td>@order.OrderDate</td>
                <td>@order.FirstName</td>
                <td>@order.LastName</td>
                <td>@order.Address</td>
                <td>@order.City</td>
                <td>@order.State</td>
                <td>@order.PostalCode</td>
                <td>@order.Country</td>
                <td>@order.Phone</td>
                <td>@order.Email</td>
                <td>@order.Total</td>

            </tr>  
        }

    </table>
</div>


<div id="logout">
    <a href="@Url.Action("LogOff", "Account")"> Log Off </a>
</div>